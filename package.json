{
  "name": "react-mini-hooks",
  "version": "1.0.0",
  "author": "Timoth√©e Astier <timothee.astier@essec.edu> (https://github.com/TimAstier)",
  "description": "Reusable hooks for react components",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "flow": "flow",
    "prettier": "prettier --write \"**/*.+(js|jsx|json|md)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different && npm run flow"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimAstier/react-mini-hooks.git"
  },
  "keywords": [
    "react",
    "hooks",
    "custom"
  ],
  "bugs": {
    "url": "https://github.com/TimAstier/react-mini-hooks/issues"
  },
  "homepage": "https://github.com/TimAstier/react-mini-hooks#readme",
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.8.0",
    "eslint-config-prettier": "3.1.0",
    "flow-bin": "0.84.0",
    "husky": "1.1.2",
    "prettier": "1.14.3"
  }
}
