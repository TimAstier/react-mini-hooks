{
  "name": "react-mini-hooks",
  "version": "0.0.0-development",
  "author": "Timoth√©e Astier <timothee.astier@essec.edu> (https://github.com/TimAstier)",
  "description": "Reusable hooks for react components",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "commit": "git-cz",
    "lint": "eslint src",
    "prettier": "prettier --write \"**/*.+(js|jsx|json|md)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different && npm run test",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TimAstier/react-mini-hooks.git"
  },
  "keywords": [
    "react",
    "hooks",
    "custom"
  ],
  "bugs": {
    "url": "https://github.com/TimAstier/react-mini-hooks/issues"
  },
  "homepage": "https://github.com/TimAstier/react-mini-hooks#readme",
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.8.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-jest": "21.26.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-hooks": "0.0.0",
    "husky": "1.1.2",
    "jest": "23.6.0",
    "jest-dom": "2.1.0",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "react-test-renderer": "16.6.0",
    "react-testing-library": "5.2.3",
    "semantic-release": "^15.10.6",
    "travis-deploy-once": "^5.0.9"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0"
  }
}
