{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "rules": {
    "react-hooks/rules-of-hooks": "error"
  },
  "env": {
    "browser": true,
    "node": true,
    "amd": true,
    "jest/globals": true
  },
  "plugins": ["jest", "react-hooks"]
}
